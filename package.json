{
  "name": "csv-to-elastic",
  "version": "1.0.0",
  "description": "Parse some CSV into elasticsearch, adding sentiment analysis",
  "main": "index.js",
  "scripts": {
    "start": "npm run build; node lib/index.js",
    "dev": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
  },
  "keywords": [
    "elasticsearch",
    "sentiment",
    "csv"
  ],
  "author": "Cameron Chamberlain",
  "license": "GPL3+",
  "dependencies": {
    "@elastic/elasticsearch": "^6.8.2",
    "commander": "^7.0.0",
    "csvtojson": "^2.0.10",
    "lodash": "^4.17.17",
    "sentiment": "^5.0.2",
    "stemmer": "^1.0.4"
  },
  "devDependencies": {
    "@types/csvtojson": "1.1.5",
    "@types/elasticsearch": "5.0.37",
    "@types/lodash": "4.14.170",
    "@types/node": "12.20.15",
    "@types/stemmer": "1.0.2",
    "nodemon": "2.0.7",
    "ts-node": "9.1.1",
    "typescript": "3.9.9"
  },
  "renovate": {
    "extends": [
      "@ahmdigital"
    ]
  },
  "engines": {
    "node": "^12",
    "npm": "6.14.13"
  }
}
